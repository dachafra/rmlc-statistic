<TriplesMap2016JAN>
  # a rr:TriplesMap;

  rr:logicalTable [
    rr:tableName "\"2016-P21\"";
  ];

  rr:subjectMap [
    a rr:Subject;
    rr:template "http://w3id.org/sri-lanka/tourism/values/arrivals-country-year-month/2016JAN-From-{COUNTRYOFRESIDENCE}";
    rr:termType rr:IRI;
    rr:class qb:Observation;
  ];

  rr:predicateObjectMap[
    rr:predicate qb:dataSet;
    rr:objectMap [
      rr:constant "sltsd:arrivals-month-by-residence";
    ];
  ];

  rr:predicateObjectMap[
    rr:predicate sltsv:countryOfResidence;
    rr:objectMap [
    rr:termType rr:IRI;
      rr:template "http://dbpedia.org/resource/{COUNTRYOFRESIDENCE}";
    ];
  ];
  
  rr:predicateObjectMap[
	rr:predicate sltsv:year;
	rr:objectMap [
		rr:termType rr:Literal;
		rr:constant "2016";
		rr:datatype xsd:gYear;
    ];
  ];
  
  rr:predicateObjectMap[
	rr:predicate sltsv:month;
	rr:objectMap [
    rr:termType rr:IRI;
    rr:constant "http://reference.data.gov.uk/def/intervals/JAN";
    ];
  ];
  
  rr:predicateObjectMap[
	rr:predicate sltsv:numberOfArrivals;
	rr:objectMap [
		rr:termType rr:Literal;
		rr:column "JAN";
		rr:datatype xsd:integer;
    ];
  ];
.

